<tal:block metal:use-macro="here/page_macros/old-new">
<tal:block metal:fill-slot="contentPane"
    tal:define="editable here/isManager">
<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value string:editMibModule"/>


        <tal:block tal:define="message request/message | string:State at time:; 
                        tabletitle string:${message} ${here/getNowString}:">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <tr>
        <td class="tableheader">Name</td>
        <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="newId" size="40"
                tal:attributes="value here/id" />
        </td>
    </tr>
    <tr>
    <td class="tableheader">Language</td>
        <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="language" size="40"
                tal:attributes="value here/language" />
        </td>
    </tr>
    <tr>
    <td class="tableheader" colspan="2">Contact</td>
    </tr>
    <tr>
    <td class="tablevalues" colspan="2">
        <textarea class="tablevalues" rows="10" cols="80" name="contact:text"
            tal:content="here/contact">
        </textarea>
    </td>
    </tr>
    <tr>
    <td class="tableheader" colspan="2">Description</td>
    </tr>
    <tr>
    <td class="tablevalues" colspan="2">
        <textarea class="tablevalues" rows="10" cols="80" name="description:text"
            tal:content="here/description">
        </textarea>
    </td>
    </tr>
    <tr>
        <td class="tableheader" colspan="2">
            <input class="tableheader" type="submit" value="Save"
                name="zmanage_editProperties:method" />
        </td>
    </tr>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
</tal:block>
</tal:block>
