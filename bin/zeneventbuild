#!/bin/sh

if [ "$ZENHOME" = "" ]; then 
    echo ERROR: ZENHOME envrionment variable not set.
    exit 1
fi

EVTDB=$ZENHOME/Products/ZenEvents/db

if [ -d $EVTDB ]; then 
    cd $EVTDB
else
    echo ERROR: $EVTDB not installed. 
    exit 1
fi

mysql -u $1 --password=$2 < zenevents.sql
mysql -u $1 --password=$2 < zenprocs.sql
for f in *.dat
do
    mysql -u $1 --password=$2 < $f
done
